
#include <iostream>
using namespace std;

class students{
public:
   string name;
   int age;
   double grade;
};

int main() {
    students student1;
    student1.name="John";
    student1.age=27;
    student1.grade= 8.2;

    students student2;
    student2.name = "Peter";
    student2.age = 23;
    student2.grade = 7.7;

    cout << student1.name<<endl;
    
    return 0;
}
